{
  "name": "aurora",
  "version": "0.17.0",
  "description": "wiki software",
  "main": "src/index.js",
  "scripts": {
    "build": "NODE_ENV=LOCAL node src/build.js",
    "build:filtered": "NODE_ENV=LOCAL node src/build.js --path-match",
    "build:test": "NODE_ENV=LOCAL node test-build-env.js",
    "debug": "NODE_ENV=LOCAL nodemon --inspect src/index.js ",
    "debug-break": "NODE_ENV=LOCAL nodemon --inspect --debug-brk src/index.js",
    "start": "NODE_ENV=LOCAL nodemon src/index.js",
    "start:pm2": "pm2 start pm2.config.json",
    "test": "echo \"tests are broken and will corrupt your mongo data\"",
    "test-broken": "mocha test/.setup.js --recursive src/**/*.test.js",
    "test:cover": "istanbul cover _mocha -- -u exports -R spec --require ignore-styles test/.setup.js test/.setup.js --recursive src/**/*.test.js -r mock-local-storage",
    "test:watch": "mocha --require ignore-styles test/.setup.js --recursive src/**/*.test.js --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.2",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.8",
    "ejs": "^2.5.6",
    "express": "^4.15.3",
    "express-nobots": "^1.0.5",
    "express-rate-limit": "^3.1.1",
    "express-session": "^1.15.3",
    "fast-glob": "^3.2.1",
    "file-type": "^17.1.1",
    "fs-extra": "^5.0.0",
    "isbinaryfile": "^5.0.0",
    "lodash": "^4.17.4",
    "markdown": "^0.5.0",
    "markdown-it": "^11.0.0",
    "markdown-it-container": "^2.0.0",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-front-matter": "^0.2.4",
    "markdown-it-sup": "^1.0.0",
    "morgan": "^1.8.2",
    "mysql": "^2.15.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "pm2": "^5.3.0",
    "recursive-readdir": "^2.2.2",
    "showdown": "^1.8.7"
  },
  "devDependencies": {
